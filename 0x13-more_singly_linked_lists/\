#include "lists.h"
/**
*add_nodeint_end - adds node at end of list
*@head: head node of list
*@n: data to be added
*Return: returns pointer to node
*/
listint_t *add_nodeint_end(listint_t **head, const int n)
{
	listint *newnode;

	newnode = malloc(sizeof(listint_t));
	if (newnode == NULL)
		return (NULL);

	newnode->n = n;
	newnode->next = NULL;

	if (head == NULL)
		return (newnode);

	while (head->next != NULL)
		head = head->next;

	head->next = *newnode;

	return (newnode);
}
